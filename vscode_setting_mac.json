{
  "window.zoomLevel": 0,
  "window.openFilesInNewWindow": "on",
  "files.trimTrailingWhitespace": true,
	"files.insertFinalNewline": true,

  "explorer.openEditors.visible": 0,

  "workbench.colorTheme": "Peacocks In Space",
  "workbench.statusBar.visible": true,
  "workbench.iconTheme": "eq-material-theme-icons",

  "markdown.preview.lineHeight": 1.4,

  "editor.quickSuggestionsDelay": 15,
  "explorer.confirmDragAndDrop": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.lineHeight": 24,
  "editor.tabCompletion": "on",
  "editor.lineNumbers": "relative",
  "editor.fontFamily": "Roboto Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.renderWhitespace": "all",

  "[php]": {
      "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  "importCost.typescriptExtensions": [
      "\\.vue$"
  ],
  "breadcrumbs.enabled": false,

  // VimVsCode configuration
  "vim.incsearch": true,
  "vim.useCtrlKeys": false,
  "vim.searchHighlightColor": "rgba(182, 196, 120, 0.5)",
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(182, 196, 120, 0.5)",
  "vim.highlightedyank.duration": 100,
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "<space>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [" "],
      "after": ["<leader>", "<leader>", "<leader>", "b", "d", "w"]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
  ],
  "vim.leader": ",",
  "vim.easymotion": true,
  // "vim.easymotionMarkerFontFamily": "Roboto Mono",
  // "vim.easymotionMarkerFontSize": "14",
  "vim.easymotionMarkerWidthPerChar": 8,
  "vim.easymotionMarkerBackgroundColor": "red",
  // "vim.easymotionMarkerHeight": 20,
  // "vim.easymotionMarkerYOffset": 4,
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "php-cs-fixer.lastDownload": 1594304395723,
  "php-cs-fixer.config": "/Users/vuongtran/.phpcsfixer",
  "editor.renameOnType": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.activityBar.visible": false,
  "workbench.editor.showTabs": true,
  // custom bar
  "workbench.colorCustomizations": {
    "statusBar.background": "#333",
    "statusBar.foreground": "#fff",
    "statusBar.noFolderBackground": "#333",
    "statusBar.noFolderForeground": "#fff"
}
  // end custom bar

  // end vim configuration


  // prettier setting
}
